<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="540.0" prefWidth="960.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jandryespol.loteria.NombreController">
    <children>
        <Label alignment="TOP_CENTER" text="Ingrese nombre de jugador">
         <font>
            <Font size="48.0" />
         </font></Label>
      <TextField>
         <font>
            <Font size="48.0" />
         </font>
      </TextField>
        <Button text="OK">
         <font>
            <Font size="48.0" />
         </font></Button>
      <Button alignment="CENTER_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" text="Cancelar">
         <font>
            <Font size="48.0" />
         </font>
      </Button>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
